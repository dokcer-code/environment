server {
    listen 171.255.255.8:80;
    server_name vse-avtoservisy.loc www.vse-avtoservisy.loc;
    return https://vse-avtoservisy.loc;
}
server {
	listen 171.255.255.8:443 ssl;
    server_name www.vse-avtoservisy.loc;
    include snippets/snakeoil.conf;
    return https://vse-avtoservisy.loc;
}
server {
	listen 171.255.255.8:443 ssl;
    server_name vse-avtoservisy.loc;
    root /var/www/vse-avtoservisy.ru/app/web;
    index index.php;
    charset utf-8;
    access_log /var/log/nginx/vse-avtoservisy.access.log;
    error_log /var/log/nginx/vse-avtoservisy.error.log;

    include snippets/snakeoil.conf;
    include snippets/favicon.ico.conf;
    include snippets/common.conf;

    include snippets/php-7.1.conf;
}
