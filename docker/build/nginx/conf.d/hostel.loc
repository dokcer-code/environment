server {
	listen 171.255.255.8:80;
	server_name hostel.loc;
	root /var/www/network-hostel/web;
    index index.php;
    charset utf-8;

    access_log /var/log/nginx/hostel.access.log;
    error_log /var/log/nginx/hostel.error.log;

    include snippets/favicon.ico.conf;
    include snippets/common.conf;
    include snippets/php-7.3.conf;
}